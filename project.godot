; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Object",
"class": "Medium",
"language": "NativeScript",
"path": "res://lib/Medium.gdns"
}, {
"base": "AcceptDialog",
"class": "MessageDialog",
"language": "GDScript",
"path": "res://ui/MessageDialog.gd"
}, {
"base": "Node",
"class": "Project",
"language": "NativeScript",
"path": "res://lib/Project.gdns"
}, {
"base": "Object",
"class": "User",
"language": "NativeScript",
"path": "res://lib/User.gdns"
} ]
_global_script_class_icons={
"Medium": "",
"MessageDialog": "",
"Project": "",
"User": ""
}

[application]

config/name="sbv-gd"
config/description="The schiller library software."
run/main_scene="res://ui/Main.tscn"
boot_splash/image="res://icons/icon.png"
boot_splash/fullsize=false
config/icon="res://icons/icon.png"
config/macos_native_icon="res://icons/icon.icns"
config/windows_native_icon="res://icons/icon.ico"

[autoload]

Project="*res://lib/Project.gdns"

[display]

window/size/width=1200
window/size/height=800
window/dpi/allow_hidpi=true

[gui]

theme/custom="res://ui/dark.tres"

[locale]

locale_filter=[ 0, [ "de", "en" ] ]
translations=PoolStringArray( "res://translations/translations.de.translation", "res://translations/translations.en.translation" )

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
